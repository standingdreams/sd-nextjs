{
  "extends": ["next/core-web-vitals", "wesbos"],
  "rules": {
    "no-console": [
      "warn", { "allow": ["warn", "error", "info"] }
    ],
    "react/jsx-props-no-spreading": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "semi": false
      }
    ]
  }
}
